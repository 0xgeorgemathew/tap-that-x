{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": [
        "corepack enable",
        "corepack prepare yarn@3.2.3 --activate",
        "yarn workspaces focus @se-2/nextjs --production=false"
      ]
    },
    "build": {
      "inputs": [
        {
          "step": "install"
        }
      ],
      "commands": [
        "yarn workspace @se-2/nextjs build"
      ]
    }
  },
  "deploy": {
    "startCommand": "yarn workspace @se-2/nextjs serve"
  }
}